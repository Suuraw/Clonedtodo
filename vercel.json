{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/feedback/:id",
      "dest": "/api/feedback/:id"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
